{
    "name": "kasm-kerbal-os",
    "displayName": "KASM (Kerbal OS)",
	"author": "Luke Newcomb",
    "description": "VSCode client for the KASM language server",
	"license": "GPL-3.0",
	"publisher": "LukeNewcomb",
	"repository": {
		"type": "git",
		"url": "https://github.com/newcomb-luke/kasm-language-server"
	},
    "version": "0.3.1",
	"icon": "icons/kasm-logo.png",
    "engines": {
        "vscode": "^1.63.0"
    },
	"keywords": [
		"KOS",
		"language-server",
		"KSP",
		"KASM",
		"Kerbal"
	],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "kasm",
            "aliases": ["Kerbal Assembly", "kasm"],
            "extensions": [".kasm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "kasm",
            "scopeName": "source.kasm",
            "path": "./syntaxes/kasm.tmLanguage.json"
        }]
    }
}